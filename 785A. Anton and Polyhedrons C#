using System;
using System.Text;

namespace ProblemSolveingRound
{
    class Program
    {
        static void Main(string[] args)
        {
            int counter = 0;

            var num = Console.ReadLine();
            int num_Shaps = int.Parse(num);

            for (int i = 0; i < num_Shaps; i++)
            {
                var name = Console.ReadLine();
                /*
                                if (name== "Tetrahedron")
                                {
                                    counter += 4;
                                }
                                else if(name== "Cube") 
                                {
                                    counter += 6;
                                }
                                else if (name == "Octahedron")
                                {
                                    counter += 8;
                                }
                                else if (name == "Dodecahedron")
                                {
                                    counter += 12;
                                }
                                else
                                {
                                    counter += 20;
                                }
                */
                switch (name)
                {
                    case "Tetrahedron":
                        counter += 4;
                        break;
                    case "Cube":
                        counter += 6;
                        break;
                    case "Octahedron":
                        counter += 8;
                        break;
                    case "Dodecahedron":
                        counter += 12;
                        break;
                    case "Icosahedron":
                        counter += 20;
                        break;
                }
            }
            Console.WriteLine(counter);




















        }
       
           






    }





    
}
