#include <iostream>
#include <string>
using namespace std ;
 
int main ()
{
	int count;
	int f1,f2,f3;
	cin>>count;
	int counter=0;
	for(int i=0;i<count;i++)
   {    
        
		cin >>f1>>f2>>f3;
		if(f1==1&&f2==1||f1==1&&f3==1||f2==1&&f3==1)
         {
              counter++;
         }
		
		
 
 
   }
	cout<< counter ;
 
 
 
	system("pasue");
 
 
	return 0;
}
