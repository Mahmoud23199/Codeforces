using System.Collections;

namespace ProblemSolveing
{
    class TestClass
    {
        static void Main(string[] args)
        {
            var input = Console.ReadLine();
            var temp =input.Split(' ');

            int num=int.Parse(temp[0]);
            int k =int.Parse(temp[1]);

            int t1;
            for (int i = 0; i < k; i++)
            {
                t1 = num % 10;

                if (t1==0)
                {
                    num=num/10;

                }
                else 
                {
                 num = num-1;
                
                }

            }
            Console.WriteLine(num);
        }
        
    }
    
}
